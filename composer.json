{
  "name" : "joskoomen/wp-cli",
  "description" : "Wordpress CLI package.",
  "version" : "2.1.0",
  "type" : "package",
  "license" : "MIT",
  "keywords" : [
    "wordpress",
    "joskoomen",
    "php"
  ],
  "extra" : {
    "hot-sauce-zip" : "https://hot-sauce.joskoomen.nl/hot-sauce-wp.zip"
  },
  "authors" : [
    {
      "name" : "Jos Koomen",
      "email" : "ik@joskoomen.nl"
    }
  ],
  "autoload" : {
    "psr-4" : {
      "JosKoomen\\Wordpress\\Cli\\" : "src/"
    }
  },
  "require" : {
    "ext-json" : "*",
    "ext-zip" : "*",
    "php" : "^7.4||^8.0",
    "symfony/console" : "^6.0",
    "symfony/filesystem" : "^6.0",
    "symfony/process" : "^6.0",
    "symfony/dom-crawler": "^5.2",
    "symfony/css-selector": "^5.2",
    "guzzlehttp/guzzle" : "^6.3",
    "vlucas/phpdotenv" : "^3.4",
    "wp-cli/wp-cli" : "^2.4",
    "wp-cli/core-command" : "^2.0",
    "wp-cli/entity-command" : "^2.0",
    "wp-cli/extension-command" : "^2.0",
    "psr/log" : "^1.1"
  },
  "require-dev" : {
    "roave/security-advisories" : "dev-latest",
    "nesbot/carbon": "^2.50"
  }
}
