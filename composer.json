{
  "name" : "ypa/wp-cli",
  "description" : "Wordpress CLI package.",
  "version" : "1.5.0",
  "type" : "package",
  "license" : "MIT",
  "keywords" : [
    "wordpress",
    "ypa"
  ],
  "authors" : [
    {
      "name" : "Jos Koomen",
      "email" : "jos@yourpersonalagency.nl"
    }
  ],
  "autoload" : {
    "psr-4" : {
      "Ypa\\Wordpress\\Cli\\" : "src/"
    }
  },
  "require" : {
    "ext-json" : "*",
    "ext-zip" : "*",
    "php" : "^7.4||^8.0",
    "symfony/console" : "^4.4",
    "symfony/filesystem" : "^4.4",
    "symfony/process" : "^4.4",
    "symfony/dom-crawler": "^5.2",
    "symfony/css-selector": "^5.2",
    "guzzlehttp/guzzle" : "^6.3",
    "vlucas/phpdotenv" : "^3.4",
    "wp-cli/wp-cli" : "^2.4",
    "wp-cli/core-command" : "^2.0",
    "wp-cli/entity-command" : "^2.0",
    "wp-cli/extension-command" : "^2.0",
    "psr/log" : "^1.1"
  },
  "require-dev" : {
    "roave/security-advisories" : "dev-latest",
    "nesbot/carbon": "^2.50"
  }
}
